[diff]
    tool = p4merge
[difftool "p4merge"]
    cmd = "p4merge.exe $LOCAL $REMOTE"
[difftool "emacs"]
    cmd = e:/Tools/Emacs/bin/ediff.sh $LOCAL $REMOTE
[merge]
	tool = p4merge
[mergetool "p4merge"]
	cmd = p4merge.exe /\"$BASE/\" /\"$LOCAL/\" /\"$REMOTE/\" /\"$MERGED/\"
[core]
	editor = e:/Tools/Emacs/bin/emacs.sh

